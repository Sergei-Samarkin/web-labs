{
  "name": "web-labs",
  "version": "1.0.0",
  "description": "",
  "main": "backend/dist/index.js",
  "scripts": {
    "dev": "cd backend && nodemon --exec ts-node -r tsconfig-paths/register src/index.ts",
    "start": "cd backend && node -r tsconfig-paths/register -r module-alias/register dist/index.js",
    "build": "cd backend && tsc",
    "tsc": "tsc",
    "eslint": "eslint",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "check": "tsc --noEmit && eslint .",
    "lint-staged": "lint-staged",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sergei-Samarkin/web-labs.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Sergei-Samarkin/web-labs/issues"
  },
  "_moduleAliases": {
    "@": "./backend/src"
  },
  "homepage": "https://github.com/Sergei-Samarkin/web-labs#readme",
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.21",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.32.1"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  }
}
